{
  "name": "tsyne-terminal",
  "version": "0.1.0",
  "description": "Terminal emulator ported to Tsyne",
  "private": true,
  "scripts": {
    "test": "npm run test:logic && npm run test:gui",
    "test:logic": "jest --testMatch='**/*.logic.test.ts' --testPathIgnorePatterns='/node_modules/' --passWithNoTests",
    "test:gui": "jest --maxWorkers=1",
    "test:watch": "jest --watch"
  },
  "devDependencies": {
    "@types/jest": "^29.5.0",
    "@types/node": "^20.0.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.4.6",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@msgpack/msgpack": "^3.0.0",
    "node-pty": "^1.0.0"
  }
}
